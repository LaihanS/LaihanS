    private void TablausuarioMouseClicked(java.awt.event.MouseEvent evt) {                                          

        int fl = Tablausuario.getSelectedRow();
        if (fl != -1) {
            int id = Integer.parseInt((String) Tablausuario.getValueAt(fl, 5).toString());
            String usua= (String) Tablausuario.getValueAt(fl, 0).toString();
            String nomb = (String) Tablausuario.getValueAt(fl, 1).toString();
            String apell = (String) Tablausuario.getValueAt(fl, 2).toString();
            String corre = (String) Tablausuario.getValueAt(fl, 3).toString();
            String telef= (String) Tablausuario.getValueAt(fl, 4).toString();
            
            nombre.setText(nomb);
            apellido.setText(apell);
            correo.setText(corre);
            notelefono.setText(telef);
            usuario.setText(usua);
            

        } else {

            JOptionPane.showMessageDialog(null, "No hay fila seleccionada", "Error", JOptionPane.INFORMATION_MESSAGE);
            deleteusers();

        }     
    }                                         

