   void Obtener_users(){
         String query;
        
                query = "SELECT ID, usuario, nombre, apellido, telefono, correo from usuario";
		   MySQLConexion con = new MySQLConexion(); 
                   con.getConexion();
                   Object[] Usuario = new Object[6];
                   tabla = (DefaultTableModel) Tablausuario.getModel();
        try {
            
             con.sttment.execute(query);
             con.rs = con.sttment.executeQuery(query);
             
             while(con.rs.next()){
                
                 Usuario[0] = con.rs.getString("usuario");
                 Usuario[1] = con.rs.getString("nombre");
                 Usuario[2] = con.rs.getString("apellido");
                 Usuario[3] = con.rs.getString("telefono");
                 Usuario[4] = con.rs.getString("correo"); 
                 Usuario[5] = con.rs.getInt("ID");
                 tabla.addRow(Usuario);
             }
             
             
        } catch (SQLException ex) {
            Logger.getLogger(Usuarios.class.getName()).log(Level.SEVERE, null, ex);
        }
   }

   
   void eliminarus(){
             MySQLConexion con = new MySQLConexion(); 
                   con.getConexion();
       int fl = Tablausuario.getSelectedRow(); 
             if(fl != -1){
                    try {
                     int id =  Integer.parseInt((String)Tablausuario.getValueAt(fl, 5).toString());
                        
                  String query = "Delete from usuario where ID = " + id;
                     con.sttment.executeUpdate(query);
                     deleteusers();
                 } catch (SQLException e) {
                     JOptionPane.showMessageDialog(null,e.getMessage());
                 }
                 
                 		
             }  else{
 
              
                 JOptionPane.showMessageDialog(null,"No hay fila seleccionada", "Error", JOptionPane.INFORMATION_MESSAGE);
	          deleteusers();
        
                   }           
   }
   
   
    void datos() {
        MySQLConexion con = new MySQLConexion();
        con.getConexion();
        int fl = Tablausuario.getSelectedRow();
        if (fl != -1) {
            int id =  Integer.parseInt((String)Tablausuario.getValueAt(fl, 5).toString());        
            String usua= usuario.getText();
            String nomb = nombre.getText();
            String apell = apellido.getText();
            String corre = correo.getText();
            String telef= notelefono.getText();
            

            String query = "UPDATE usuario set nombre ='" + nomb+ "', usuario = '" + usua + "',apellido ='" + apell+"', telefono ='" + telef +"',correo ='" +corre+ "' where ID =" + id;
            try {
                con.sttment.executeUpdate(query);
            } catch (SQLException ex) {
                Logger.getLogger(Usuarios.class.getName()).log(Level.SEVERE, null, ex);
            }
            

            deleteusers();

        } else {

            JOptionPane.showMessageDialog(null, "No hay fila seleccionada", "Error", JOptionPane.INFORMATION_MESSAGE);
            deleteusers();

        }     
   }
    
    
    
