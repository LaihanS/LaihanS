    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         

        MySQLConexion con = new MySQLConexion();
        Connection conec = con.getConexion();
        boolean contra = false;
        jTextField1.setName("Apellido");
        jTextField2.setName("Usuario");
        jTextField3.setName("Nombre");
        jTextField4.setName("Notelefono");
        jTextField8.setName("Correo");
        jPasswordField2.setName("Contraseña");
        jPasswordField1.setName("Vercontra");
        jTextField5.setName("Extra");

        javax.swing.JTextField[] emptyd = {jTextField2, jTextField3, jTextField1, jTextField4, jTextField8, jPasswordField2, jPasswordField1, jTextField5};
        List<String> vacios = new ArrayList<>();
        String query;
        query = "insert into usuario (usuario,nombre,apellido,telefono,correo,contraseña) values (?,?,?,?,?,?)";
        String query2 = "Select contraseña from usuario";

        try {
            PreparedStatement est = conec.prepareStatement(query);

            con.sttment.execute(query2);
            con.rs = con.sttment.executeQuery(query2);
            while (con.rs.next()) {
                if (con.rs.getString("contraseña").equals(jPasswordField2.getText())) {
                    JOptionPane.showMessageDialog(null, "Error. Ya existe un usuario cone esta contraseña", "Error", JOptionPane.INFORMATION_MESSAGE);
                    contra = true;
                    break;
                }
            }
            
            if(!contra){
  
            for (javax.swing.JTextField dat : emptyd) {
                if (dat.getText().isEmpty() && !"Extra".equals(dat.getName())) {
                    vacios.add(dat.getName());
                }
            }
            if (!vacios.isEmpty()) {
                if (vacios.size() == 1) {
                    JOptionPane.showMessageDialog(null, "El campo " + vacios + " esta vacio", "Error", JOptionPane.INFORMATION_MESSAGE);
                } else {
                    JOptionPane.showMessageDialog(null, "Los campos " + vacios + " estan vacios", "Error", JOptionPane.INFORMATION_MESSAGE);
                }

            } else {
                if (jPasswordField2.getText().equals(jPasswordField1.getText()) && jPasswordField2.getText().length() >= 8) {

                    for (int i = 0; i < emptyd.length - 2; i++) {
                        est.setString(i + 1, emptyd[i].getText());
                    }
                    login log = new login();
                    est.execute();
                    this.dispose();
                    log.setVisible(true);

                } else if (jPasswordField2.getText().length() < 8) {
                    JOptionPane.showMessageDialog(null, "Longitud de contraseña inadecuada (" + jPasswordField2.getText().length() + ") de 8.", "Resultado", JOptionPane.INFORMATION_MESSAGE);
                } else {
                    JOptionPane.showMessageDialog(null, "Contraseñas distintas", "Resultado", JOptionPane.INFORMATION_MESSAGE);
                }

            }
            
           } 

        } catch (SQLException e) {
            JOptionPane.showMessageDialog(null, e.getMessage());
    }                                        
    }
    private void jCheckBox1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        if (jCheckBox1.isSelected()) {
            jPasswordField2.setEchoChar((char) 0);
        } else {
            jPasswordField2.setEchoChar('*');
        }

    }                                          

    private void jCheckBox2ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        if (jCheckBox2.isSelected()) {
            jPasswordField1.setEchoChar((char) 0);
        } else {
            jPasswordField1.setEchoChar('*');
        }
    }                                          

    private void jPasswordField2ActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
    }                                               

